  "scripts": {
    "@ABBR": "------------------------------------------------------------------------------------",
    "b": "yarn build",
    "d": "yarn dev",
    "@PROD": "------------------------------------------------------------------------------------",
    "build": "webpack",
    "start": "node ./build/main.js",
    "@DEV": "-------------------------------------------------------------------------------------",
    "build:watch": "webpack --watch",
    "start:watch": "nodemon --watch ./build ./build/main.js",
    "dev": "run-p build:watch start:watch",
    "@DOCKER": "----------------------------------------------------------------------------------",
    "docker:build": "docker build -t its__ms_template .",
    "@LINTER": "----------------------------------------------------------------------------------",
    "lint:check": "yarn prettier:check",
    "lint:fix": "yarn prettier:fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "dotenv": "^14.3.2",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
