{
  "name": "its__ms_template",
  "license": "MIT",
  "author": {
    "name": "Sergey Dunaevskiy",
    "email": "sergey@dunaevskiy.dev",
    "url": "https://dunaevskiy.dev"
  },
  "scripts": {
    "@ABBR": "------------------------------------------------------------------------------------",
    "b": "yarn build",
    "d": "yarn dev",
    "@PROD": "------------------------------------------------------------------------------------",
    "build": "webpack",
    "start": "node ./build/main.js",
    "@DEV": "-------------------------------------------------------------------------------------",
    "build:watch": "webpack --watch",
    "start:watch": "nodemon --watch ./build ./build/main.js",
    "dev": "run-p build:watch start:watch",
    "@DOCKER": "----------------------------------------------------------------------------------",
    "docker:build": "docker build -t its__ms_template .",
    "@LINTER": "----------------------------------------------------------------------------------",
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:check": "eslint --ext .ts,.tsx .",
    "eslint:fix": "eslint --ext .ts,.tsx --fix .",
    "eslint:staged": "eslint --ext .ts,.tsx --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:staged": "prettier --write --ignore-unknown",
    "@OTHER": "-----------------------------------------------------------------------------------",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^14.3.2",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
